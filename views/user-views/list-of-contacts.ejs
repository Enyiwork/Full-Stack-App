<!-- Profile Texts -->
<div class="div-profile">

  <button type="button" class="button-plus" data-container="body" data-toggle="popover" data-placement="right">
    <img class="img-plus-btn"src="https://cdn2.iconfinder.com/data/icons/picons-essentials/71/user_add-512.png" />
  </button>

  <button class="list-contacts" href="my-list-contacts" data-container="body" data-toggle="popover" data-placement="botton">List Contacts</button>

     <% if (currentUser.role === "admin"){ %>

       <a class="all-users" href="/admin/users">All Users</a>
       <a class="all-contacts" href="/admin/contacts">Users Contacts</a>

     <% } %>


    <div id="btn-plus">
      <ul>


        <h2 class="contact-title">New Contact</h2>

        <form class="add-contacts" method="post" action="/contact">


          <input id="first-name" type="text" placeholder="First Name"
          name="firstN">

          <input id="last-name" type="text" placeholder="Last Name"
          name="lastN">

          <br />

          <input id="phone" type="text" placeholder="Phone Number"
          name="phoneN">
          <br />

          <input id="email" type="email" placeholder="Email Address"
          name="emailN">
          <br />

          <input id="email" type="text" placeholder="Address"
          name="addressN">
          <br />

          <button class="save-btn"> Save </button>
        </form>


      </ul>
    </div>




    <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
      <div class="panel-body">
        <ul class="contact-list">
          <% listOfContacts.forEach((oneContact) => { %>
            <li style="margin-right: 5%; margin-bottom: 5%; font-size: 20px; color: #FFF;">
            <h4 class="line-down">Name: <%= oneContact.firstname +" "+ oneContact.lastname %> </h4>
            <p class="line-down">Phone Number:  <%= oneContact.phone %> </p>
            <p class="line-down">Eamil Address:  <%= oneContact.email %> </p>
            <p class="line-down">Address:  <%= oneContact.address %> </p>
               <ul>
                  <li>
                  <a class="edit line-down" href="my-list-contacts/<%= oneContact._id %>/edit">
                    Edit Contact
                  </a>
                  </li>
               </ul>
            </li>
            <% }) %>
          </ul>
        </div>
      </div>






</div>
