<div class="div-profile">
<button class="list-contacts" href="/admin/contacts" data-container="body" data-toggle="popover" data-placement="botton">Users Contacts</button>

<h2> All Contacts </h2>


<ul>
  <% listOfContacts.forEach((oneContact) => { %>
   <li>
      <h3> <%= oneContact.firstname +" "+ oneContact.lastname %> </h3>

      <p> <%= oneContact.phone %> </p>
      <p> <%= oneContact.email %> </p>
      <p> <%= oneContact.address %> </p>
      <p>
      Owner By: <%= oneContact.owner %> </p>

     <!-- compare ObjectIds as strings (they are NOT strings by default) -->
      <% if (oneContact.owner.toString() === currentUser._id.toString()) { %>
        (this is you)
        <% } %>

   </li>
  <% }) %>
</ul>

</div>
